                },
                {
                    "name": "Cimager.gridder.WProject.oversample",
                    "value": 4,
                    "defaultValue": "8",
                    "description": "Oversampling factor. Convolution functions will be computed for this number of pixels per uv-cell.",
                    "readonly": false,
                    "type": "Integer",
                    "precious": false,
                    "options": [],

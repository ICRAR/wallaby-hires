                    "readonly": false,
                    "id": "parameter28"
                },
                {
                    "name": "Cimager.freqframe",
                    "type": "String",
                    "options": [],
                    "defaultValue": "topo",
                    "description": "Frequency frame to work in (the frame is converted when the dataset is read). Either lsrk or topo is supported.",
                    "keyAttribute": false,
                    "positional": false,
                    "precious": false,
                    "readonly": false,
